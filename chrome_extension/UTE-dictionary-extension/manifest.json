{
  "manifest_version": 3,
  "name": "Dictionary App",
  "version": "1.0",
  "description": "A simple dictionary app for quick lookups.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quick Dictionary"
  },
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}