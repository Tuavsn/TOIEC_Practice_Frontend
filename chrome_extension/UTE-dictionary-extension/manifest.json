{
  "manifest_version": 3,
  "name": "UTE App",
  "version": "1.0",
  "description": "A simple app for quick lookups.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quick App"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}